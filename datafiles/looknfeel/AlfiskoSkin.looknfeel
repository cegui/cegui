    <WidgetLook name="AlfiskoSkin/ProgressBar">
        <Property name="CurrentProgress" value=".01" />
        <NamedArea name="ProgressArea" />
        <ImagerySection name="progress_empty">
            <FrameComponent>
                <Image type="TopLeftCorner" name="AlfiskoSkin/ProgressEmptyTL" />
                <Image type="TopRightCorner" name="AlfiskoSkin/ProgressEmptyTR" />
                <Image type="BottomLeftCorner" name="AlfiskoSkin/ProgressEmptyBL" />
                <Image type="BottomRightCorner" name="AlfiskoSkin/ProgressEmptyBR" />
                <Image type="LeftEdge" name="AlfiskoSkin/ProgressEmptyL" />
                <Image type="RightEdge" name="AlfiskoSkin/ProgressEmptyR" />
                <Image type="TopEdge" name="AlfiskoSkin/ProgressEmptyT" />
                <Image type="BottomEdge" name="AlfiskoSkin/ProgressEmptyB" />
                <Image type="Background" name="AlfiskoSkin/ProgressEmptyFill" />
            </FrameComponent>
        </ImagerySection>
        <ImagerySection name="progress_full" >
            <FrameComponent>
                <Image type="TopLeftCorner" name="AlfiskoSkin/ProgressFullTL" />
                <Image type="TopRightCorner" name="AlfiskoSkin/ProgressFullTR" />
                <Image type="BottomLeftCorner" name="AlfiskoSkin/ProgressFullBL" />
                <Image type="BottomRightCorner" name="AlfiskoSkin/ProgressFullBR" />
                <Image type="LeftEdge" name="AlfiskoSkin/ProgressFullL" />
                <Image type="RightEdge" name="AlfiskoSkin/ProgressFullR" />
                <Image type="TopEdge" name="AlfiskoSkin/ProgressFullT" />
                <Image type="BottomEdge" name="AlfiskoSkin/ProgressFullB" />
                <Image type="Background" name="AlfiskoSkin/ProgressFullFill" />
            </FrameComponent>
        </ImagerySection>
        <ImagerySection name="progress_shadow" >
            <ImageryComponent>
                <Area>
                    <Dim type="LeftEdge" >
                        <UnifiedDim scale="1" type="Width">
                            <DimOperator op="Multiply">
                                <PropertyDim name="CurrentProgress" />
                            </DimOperator>
                        </UnifiedDim>
                    </Dim>
                    <Dim type="RightEdge" >
                        <UnifiedDim scale="1" type="Width">
                            <DimOperator op="Subtract">
                                <AbsoluteDim value="5" />
                            </DimOperator>
                        </UnifiedDim>
                    </Dim> 
                </Area>
                <Image name="AlfiskoSkin/ProgressShadow" />
                <VertFormat type="Stretched" />
            </ImageryComponent>
        </ImagerySection>
        <StateImagery name="Enabled">
            <Layer>
                <Section section="progress_empty" />
                <Section section="progress_shadow" />
            </Layer>
        </StateImagery>
        <StateImagery name="Disabled">
            <Layer>
                <Section section="progress_empty">
                    <Colours topLeft="FF7F7F7F" topRight="FF7F7F7F" bottomLeft="FF7F7F7F" bottomRight="FF7F7F7F" />
                </Section>
            </Layer>
        </StateImagery>
        <StateImagery name="EnabledProgress">
            <Layer>
                <Section section="progress_full" />
            </Layer>
        </StateImagery>
        <StateImagery name="DisabledProgress">
            <Layer>
                <Section section="progress_full">
                    <Colours topLeft="FF7F7F7F" topRight="FF7F7F7F" bottomLeft="FF7F7F7F" bottomRight="FF7F7F7F" />
                </Section>
            </Layer>
        </StateImagery>
    </WidgetLook>

