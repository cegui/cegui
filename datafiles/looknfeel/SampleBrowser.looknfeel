<?xml version="1.0" ?>

<!--
    SampleBrowserSkin.looknfeel by Lukas E. Meindl

    This looknfeel is created for the SampleFramework to provide a minimal
	set of widgets that are specifically designed for it. This also shows
	how the looknfeel system and imagesets can be customized specifically
	for a project.
-->

<Falagard version="7">
    <WidgetLook name="SampleBrowserSkin/Label">
        <PropertyDefinition name="NormalTextColour" initialValue="FF000000" redrawOnWrite="true" type="Colour" />
        <PropertyDefinition name="DisabledTextColour" initialValue="FF444444" redrawOnWrite="true" type="Colour" />
        <PropertyDefinition name="VertFormatting" initialValue="CentreAligned" redrawOnWrite="true" type="VerticalTextFormatting" />
        <PropertyDefinition name="HorzFormatting" initialValue="CentreAligned" redrawOnWrite="true" type="HorizontalTextFormatting" />
        <ImagerySection name="Label">
            <TextComponent>
                <Area>
                    <Dim type="LeftEdge"><AbsoluteDim value="0" /></Dim>
                    <Dim type="TopEdge"><AbsoluteDim value="0" /></Dim>
                    <Dim type="Width"><UnifiedDim scale="1" type="Width" /></Dim>
                    <Dim type="Height"><UnifiedDim scale="1" type="Height" /></Dim>
                </Area>
                <VertFormatProperty name="VertFormatting" />
                <HorzFormatProperty name="HorzFormatting" />
            </TextComponent>
        </ImagerySection>
        <StateImagery name="Enabled">
            <Layer>
                <Section section="Label">
                    <ColourProperty name="NormalTextColour" />
                </Section>
            </Layer>
        </StateImagery>
        <StateImagery name="Disabled">
            <Layer>
                <Section section="Label">
                    <ColourProperty name="DisabledTextColour" />
                </Section>
            </Layer>
        </StateImagery>
    </WidgetLook>

    <!--
:::: Shared static widget components.
-->
    <WidgetLook name="SampleBrowserSkin/StaticShared">
        <ImagerySection name="frame">
            <FrameComponent>
                <Area>
                    <Dim type="LeftEdge"><AbsoluteDim value="0" /></Dim>
                    <Dim type="TopEdge"><AbsoluteDim value="0" /></Dim>
                    <Dim type="RightEdge"><UnifiedDim scale="1" type="RightEdge" /></Dim>
                    <Dim type="BottomEdge"><UnifiedDim scale="1" type="BottomEdge" /></Dim>
                </Area>
                <Image component="TopLeftCorner" name="SampleBrowserSkin/TextBoxTL" />
                <Image component="TopRightCorner" name="SampleBrowserSkin/TextBoxTR" />
                <Image component="BottomLeftCorner" name="SampleBrowserSkin/TextBoxBL" />
                <Image component="BottomRightCorner" name="SampleBrowserSkin/TextBoxBR" />
                <Image component="LeftEdge" name="SampleBrowserSkin/TextBoxL" />
                <Image component="TopEdge" name="SampleBrowserSkin/TextBoxT" />
                <Image component="RightEdge" name="SampleBrowserSkin/TextBoxR" />
                <Image component="BottomEdge" name="SampleBrowserSkin/TextBoxB" />
            </FrameComponent>
        </ImagerySection>
        <ImagerySection name="with_frame_background">
            <ImageryComponent>
                <Area>
                    <Dim type="LeftEdge" >
                        <ImageDim name="SampleBrowserSkin/TextBoxL" dimension="Width" />
                    </Dim>
                    <Dim type="TopEdge" >
                        <ImageDim name="SampleBrowserSkin/TextBoxT" dimension="Height" />
                    </Dim>
                    <Dim type="RightEdge" >
                        <OperatorDim op="Subtract">
                            <UnifiedDim scale="1.0" type="RightEdge" />
                            <ImageDim name="SampleBrowserSkin/TextBoxR" dimension="Width" />
                        </OperatorDim>
                    </Dim>
                    <Dim type="BottomEdge" >
                        <OperatorDim op="Subtract">
                            <UnifiedDim scale="1.0" type="BottomEdge" />
                            <ImageDim name="SampleBrowserSkin/TextBoxB" dimension="Height" />
                        </OperatorDim>
                    </Dim>
                </Area>
                <Image name="SampleBrowserSkin/TextBoxM" />
                <VertFormat type="Stretched" />
                <HorzFormat type="Stretched" />
            </ImageryComponent>
        </ImagerySection>
        <ImagerySection name="no_frame_background">
            <ImageryComponent>
                <Area>
                    <Dim type="LeftEdge"><AbsoluteDim value="0" /></Dim>
                    <Dim type="TopEdge"><AbsoluteDim value="0" /></Dim>
                    <Dim type="RightEdge"><UnifiedDim scale="1" type="RightEdge" /></Dim>
                    <Dim type="BottomEdge"><UnifiedDim scale="1" type="BottomEdge" /></Dim>
                </Area>
                <Image name="SampleBrowserSkin/TextBoxM" />
                <VertFormat type="Stretched" />
                <HorzFormat type="Stretched" />
            </ImageryComponent>
        </ImagerySection>
    </WidgetLook>


    <WidgetLook name="SampleBrowserSkin/Button">
        <PropertyDefinition name="NormalTextColour"	initialValue="FFFFFFFF" redrawOnWrite="true" type="ColourRect" />
        <PropertyDefinition name="HoverTextColour" initialValue="FFFFFFFF" redrawOnWrite="true"  type="ColourRect"/>
        <PropertyDefinition name="PushedTextColour" initialValue="FFFFFFFF" redrawOnWrite="true" type="ColourRect"/>
        <PropertyDefinition name="DisabledTextColour" initialValue="FF888888" redrawOnWrite="true" type="ColourRect"/>
        <PropertyDefinition name="UseStandardImagery" initialValue="true" redrawOnWrite="true" type="bool"/>
        <PropertyDefinition name="NormalImage" initialValue="" redrawOnWrite="true" type="Image"/>
        <PropertyDefinition name="HoverImage" initialValue="" redrawOnWrite="true" type="Image"/>
        <PropertyDefinition name="PushedImage" initialValue="" redrawOnWrite="true" type="Image"/>
        <PropertyDefinition name="VertImageFormatting" initialValue="Stretched" redrawOnWrite="true" type="VerticalFormatting"/>
        <PropertyDefinition name="HorzImageFormatting" initialValue="Stretched" redrawOnWrite="true" type="HorizontalFormatting"/>

        <ImagerySection name="label">
            <TextComponent>
                <Area>
                    <Dim type="LeftEdge"><AbsoluteDim value="0" /></Dim>
                    <Dim type="TopEdge"><AbsoluteDim value="0" /></Dim>
                    <Dim type="RightEdge"><UnifiedDim scale="1" type="RightEdge" /></Dim>
                    <Dim type="BottomEdge"><UnifiedDim scale="1" type="BottomEdge" /></Dim>
                </Area>
                <VertFormat type="CentreAligned" />
                <HorzFormat type="WordWrapCentreAligned" />
            </TextComponent>
        </ImagerySection>
        <ImagerySection name="normal_image">
            <ImageryComponent>
                <Area>
                    <Dim type="LeftEdge" ><AbsoluteDim value="0" /></Dim>
                    <Dim type="TopEdge" ><AbsoluteDim value="0" /></Dim>
                    <Dim type="Width" ><UnifiedDim scale="1" type="Width" /></Dim>
                    <Dim type="Height" ><UnifiedDim scale="1" type="Height" /></Dim>
                </Area>
                <ImageProperty name="NormalImage" />
                <VertFormatProperty name="VertImageFormatting" />
                <HorzFormatProperty name="HorzImageFormatting" />
            </ImageryComponent>
        </ImagerySection>
        <ImagerySection name="hover_image">
            <ImageryComponent>
                <Area>
                    <Dim type="LeftEdge" ><AbsoluteDim value="0" /></Dim>
                    <Dim type="TopEdge" ><AbsoluteDim value="0" /></Dim>
                    <Dim type="Width" ><UnifiedDim scale="1" type="Width" /></Dim>
                    <Dim type="Height" ><UnifiedDim scale="1" type="Height" /></Dim>
                </Area>
                <ImageProperty name="HoverImage" />
                <VertFormatProperty name="VertImageFormatting" />
                <HorzFormatProperty name="HorzImageFormatting" />
            </ImageryComponent>
        </ImagerySection>
        <ImagerySection name="pushed_image">
            <ImageryComponent>
                <Area>
                    <Dim type="LeftEdge" ><AbsoluteDim value="0" /></Dim>
                    <Dim type="TopEdge" ><AbsoluteDim value="0" /></Dim>
                    <Dim type="Width" ><UnifiedDim scale="1" type="Width" /></Dim>
                    <Dim type="Height" ><UnifiedDim scale="1" type="Height" /></Dim>
                </Area>
                <ImageProperty name="PushedImage" />
                <VertFormatProperty name="VertImageFormatting" />
                <HorzFormatProperty name="HorzImageFormatting" />
            </ImageryComponent>
        </ImagerySection>

        <StateImagery name="Normal">
            <Layer>
                <Section section="normal_image" />
                <Section section="label">
                    <ColourProperty name="NormalTextColour" />
                </Section>
            </Layer>
        </StateImagery>
        <StateImagery name="Hover">
            <Layer>
                <Section section="hover_image" />
                <Section section="label">
                    <ColourProperty name="HoverTextColour" />
                </Section>
            </Layer>
        </StateImagery>
        <StateImagery name="Pushed">
            <Layer>
                <Section section="pushed_image" />
                <Section section="label">
                    <ColourProperty name="PushedTextColour" />
                </Section>
            </Layer>
        </StateImagery>
        <StateImagery name="Disabled">
            <Layer>
                <Section section="normal_image">
                    <Colours topLeft="FF3F3F3F" topRight="FF3F3F3F" bottomLeft="FF3F3F3F" bottomRight="FF3F3F3F" />
                </Section>
                <Section section="label">
                    <ColourProperty name="DisabledTextColour" />
                </Section>
            </Layer>
        </StateImagery>
    </WidgetLook>

    <WidgetLook name="SampleBrowserSkin/SampleWindow">
        <PropertyDefinition name="BackgroundEnabled" initialValue="true" redrawOnWrite="true" layoutOnWrite="true" type="bool" />
        <PropertyDefinition name="ForegroundEnabled" initialValue="true" redrawOnWrite="true" layoutOnWrite="true" type="bool" />
        <PropertyDefinition name="FrameEnabled" initialValue="true" redrawOnWrite="true" layoutOnWrite="true" type="bool" />
        <PropertyDefinition name="FadeEnabled" initialValue="true" redrawOnWrite="true" layoutOnWrite="true" type="bool" />

        <PropertyDefinition name="ImageColours" initialValue="tl:FFFFFFFF tr:FFFFFFFF bl:FFFFFFFF br:FFFFFFFF" redrawOnWrite="true" type="ColourRect"/>
        <PropertyDefinition name="FrameColours" initialValue="tl:FFFFFFFF tr:FFFFFFFF bl:FFFFFFFF br:FFFFFFFF" redrawOnWrite="true" type="ColourRect"/>
        <PropertyDefinition name="ForegroundColours" initialValue="tl:FFFFFFFF tr:FFFFFFFF bl:FFFFFFFF br:FFFFFFFF" redrawOnWrite="true" type="ColourRect"/> 
        <PropertyDefinition name="BackgroundColours" initialValue="tl:FF000000 tr:FF000000 bl:FF000000 br:FF000000" redrawOnWrite="true" type="ColourRect"/> 
        <PropertyDefinition name="ForegroundFadeColours" initialValue="tl:CC000000 tr:CC000000 bl:CC000000 br:CC000000" redrawOnWrite="true" type="ColourRect"/>
        <PropertyDefinition name="Image" initialValue="" redrawOnWrite="true" />

        <Property name="AutoRenderingSurface" value="true" />


        <NamedArea name="InnerArea">
            <Area>
                <Dim type="LeftEdge" >
                    <OperatorDim op="Multiply">
                        <ImageDim name="SampleBrowserSkin/SampleFrameLeft" dimension="Width" />
                        <PropertyDim name="FrameEnabled" />
                    </OperatorDim>
                </Dim>
                <Dim type="TopEdge" >
                    <OperatorDim op="Multiply">
                        <ImageDim name="SampleBrowserSkin/SampleFrameTop" dimension="Height" />
                        <PropertyDim name="FrameEnabled" />
                    </OperatorDim>
                </Dim>
                <Dim type="RightEdge" >
                    <OperatorDim op="Subtract">
                        <UnifiedDim scale="1.0" type="RightEdge" />
                        <OperatorDim op="Multiply">
                            <ImageDim name="SampleBrowserSkin/SampleFrameRight" dimension="Width" />
                            <PropertyDim name="FrameEnabled" />
                        </OperatorDim>
                    </OperatorDim>
                </Dim>
                <Dim type="BottomEdge" >
                    <OperatorDim op="Subtract">
                        <UnifiedDim scale="1.0" type="BottomEdge" />
                        <OperatorDim op="Multiply">
                            <ImageDim name="SampleBrowserSkin/SampleFrameBottom" dimension="Height" />
                            <PropertyDim name="FrameEnabled" />
                        </OperatorDim>
                    </OperatorDim>
                </Dim>
            </Area>
        </NamedArea>

        <ImagerySection name="frame">
            <FrameComponent>
                <Area>
                    <Dim type="LeftEdge"><AbsoluteDim value="0" /></Dim>
                    <Dim type="TopEdge"><AbsoluteDim value="0" /></Dim>
                    <Dim type="RightEdge"><UnifiedDim scale="1" type="RightEdge" /></Dim>
                    <Dim type="BottomEdge"><UnifiedDim scale="1" type="BottomEdge" /></Dim>
                </Area>
                <Image component="TopLeftCorner" name="SampleBrowserSkin/SampleFrameTopLeft" />
                <Image component="TopRightCorner" name="SampleBrowserSkin/SampleFrameTopRight" />
                <Image component="BottomLeftCorner" name="SampleBrowserSkin/SampleFrameBottomLeft" />
                <Image component="BottomRightCorner" name="SampleBrowserSkin/SampleFrameBottomRight" />
                <Image component="LeftEdge" name="SampleBrowserSkin/SampleFrameLeft" />
                <Image component="TopEdge" name="SampleBrowserSkin/SampleFrameTop" />
                <Image component="RightEdge" name="SampleBrowserSkin/SampleFrameRight" />
                <Image component="BottomEdge" name="SampleBrowserSkin/SampleFrameBottom" />
                <ColourRectProperty name="FrameColours" />
            </FrameComponent>
        </ImagerySection>

        <ImagerySection name="image">
            <ImageryComponent>
                <Area><NamedAreaSource name="InnerArea"/></Area>
                <ImageProperty name="Image" />
                <ColourRectProperty name="ImageColours" />
                <VertFormat type="Stretched" />
                <HorzFormat type="Stretched" />
            </ImageryComponent>
        </ImagerySection>

        <ImagerySection name="fadeForeground">
            <ImageryComponent>
                <Area><NamedAreaSource name="InnerArea"/></Area>
                <Image name="SampleBrowserSkin/GenericFiller" />
                <ColourRectProperty name="ForegroundFadeColours" />
                <VertFormat type="Stretched" />
                <HorzFormat type="Stretched" />
            </ImageryComponent>     
        </ImagerySection>

        <ImagerySection name="foreground">
            <ImageryComponent>
                <Area><NamedAreaSource name="InnerArea"/></Area>
                <Image name="SampleBrowserSkin/SamplesForeground" />
                <ColourRectProperty name="ForegroundColours" />
                <VertFormat type="Stretched" />
                <HorzFormat type="Stretched" />
            </ImageryComponent>     
        </ImagerySection>

        <ImagerySection name="background">
            <ImageryComponent>
                <Area><NamedAreaSource name="InnerArea"/></Area>
                <Image name="SampleBrowserSkin/GenericFiller" />
                <ColourRectProperty name="BackgroundColours" />
                <VertFormat type="Stretched" />
                <HorzFormat type="Stretched" />
            </ImageryComponent>     
        </ImagerySection>

        <StateImagery name="Enabled">
            <Layer priority="4" ><Section section="frame" controlProperty="FrameEnabled" /></Layer> 
            <Layer priority="3" ><Section section="foreground" controlProperty="ForegroundEnabled" /></Layer>
            <Layer priority="2" ><Section section="fadeForeground" controlProperty="FadeEnabled" /></Layer>
            <Layer priority="1" ><Section section="image" /></Layer> 
            <Layer priority="0" ><Section section="background" controlProperty="BackgroundEnabled" /></Layer>
        </StateImagery>
        <StateImagery name="Disabled">
        </StateImagery>


        <AnimationDefinition name="SamplePreviewFadeIn" duration="0.28" replayMode="once" autoStart="false">
            <Affector property="ForegroundFadeColours" interpolator="ColourRect">
                <KeyFrame position="0" sourceProperty="ForegroundFadeColours" />
                <KeyFrame position="0.28" value="tl:00000000 tr:00000000 bl:00000000 br:00000000" progression="quadratic decelerating" />
            </Affector>
            <Subscription event="CursorLeavesArea" action="Stop" />
            <Subscription event="CursorEntersArea" action="Start" />
        </AnimationDefinition>

        <AnimationDefinition name="SamplePreviewFadeOut" duration="0.39" replayMode="once" autoStart="false">
            <Affector property="ForegroundFadeColours" interpolator="ColourRect">
                <KeyFrame position="0" sourceProperty="ForegroundFadeColours" />
                <KeyFrame position="0.39" value="tl:CC000000 tr:CC000000 bl:CC000000 br:CC000000" progression="quadratic decelerating" />
            </Affector>
            <Subscription event="CursorEntersArea" action="Stop" />
            <Subscription event="CursorLeavesArea" action="Start" />
        </AnimationDefinition>
    </WidgetLook>

   <WidgetLook name="SampleBrowserSkin/Light">
        <PropertyDefinition name="BackgroundColour" initialValue="FFFFFFFF" redrawOnWrite="true" type="Colour"/>
        <PropertyDefinition name="LightColour" initialValue="FFFFFFFF" redrawOnWrite="true" type="Colour"/>

        <NamedArea name="LightArea">
            <Area>
                <Dim type="LeftEdge" ><AbsoluteDim value="0" /></Dim>
                <Dim type="TopEdge" ><AbsoluteDim value="0" /></Dim>
                <Dim type="Width" ><UnifiedDim scale="1" type="Width" /></Dim>
                <Dim type="Height" ><UnifiedDim scale="1" type="Height" /></Dim>
            </Area>
        </NamedArea>
        
        <ImagerySection name="background">
            <ImageryComponent>
                <Area><NamedAreaSource name="LightArea"/></Area>
                <Image name="SampleBrowserSkin/LightBackground" />
                <ColourProperty name="BackgroundColour" />
                <VertFormat type="Stretched" />
                <HorzFormat type="Stretched" />
            </ImageryComponent>     
        </ImagerySection>

        <ImagerySection name="foreground">
            <ImageryComponent>
                <Area><NamedAreaSource name="LightArea"/></Area>
                <Image name="SampleBrowserSkin/LightOverlay" />
                <ColourProperty name="LightColour" />
                <VertFormat type="Stretched" />
                <HorzFormat type="Stretched" />
            </ImageryComponent>     
        </ImagerySection>

        <StateImagery name="Enabled">
            <Layer priority="1" ><Section section="foreground" /></Layer> 
            <Layer priority="0" ><Section section="background" /></Layer>
        </StateImagery>
        <StateImagery name="Disabled">
            <Layer><Section section="background" /></Layer>
        </StateImagery>

    </WidgetLook>


    <!--
    TitleBar
-->
    <WidgetLook name="SampleBrowserSkin/Titlebar">
        <PropertyDefinition name="CaptionColour" initialValue="FFe5ffb2" redrawOnWrite="true" type="ColourRect"/>
        <PropertyDefinition name="VertFormatting" initialValue="CentreAligned" type="VerticalTextFormatting"/>
        <PropertyDefinition name="HorzFormatting" initialValue="CentreAligned" type="HorizontalTextFormatting"/>

        <Property name="NonClient" value="true" />

        <ImagerySection name="normal">
            <FrameComponent>
                <Area>
                    <Dim type="LeftEdge"><AbsoluteDim value="0" /></Dim>
                    <Dim type="TopEdge"><AbsoluteDim value="0" /></Dim>
                    <Dim type="RightEdge"><UnifiedDim scale="1" type="RightEdge" /></Dim>
                    <Dim type="BottomEdge"><UnifiedDim scale="1" type="BottomEdge" /></Dim>
                </Area>
                <Image component="LeftEdge" name="SampleBrowserSkin/CaptionL" />
                <Image component="RightEdge" name="SampleBrowserSkin/CaptionR" />
                <Image component="Background" name="SampleBrowserSkin/CaptionM" />
            </FrameComponent>
        </ImagerySection>
        <ImagerySection name="caption">
            <TextComponent>
                <Area>
                    <Dim type="LeftEdge"><AbsoluteDim value="0" /></Dim>
                    <Dim type="TopEdge"><AbsoluteDim value="0" /></Dim>
                    <Dim type="RightEdge"><UnifiedDim scale="1" type="RightEdge" /></Dim>
                    <Dim type="BottomEdge"><UnifiedDim scale="1" type="BottomEdge" /></Dim>
                </Area>
                <ColourProperty name="CaptionColour" />
                <VertFormatProperty name="VertFormatting" />
                <HorzFormatProperty name="HorzFormatting" />
            </TextComponent>
        </ImagerySection>
        <StateImagery name="Active">
            <Layer>
                <Section section="normal" />
                <Section section="caption" />
            </Layer>
        </StateImagery>
        <StateImagery name="Inactive">
            <Layer>
                <Section section="normal" />
                <Section section="caption" />
            </Layer>
        </StateImagery>
        <StateImagery name="Disabled">
            <Layer>
                <Section section="normal" />
                <Section section="caption" />
            </Layer>
        </StateImagery>
    </WidgetLook>

